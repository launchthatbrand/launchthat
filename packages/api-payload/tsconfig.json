{
  "extends": "@acme/tsconfig/internal-package.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/payload-types": ["../payload-cms/src/payload-types"],
      "@/components/*": ["../payload-cms/src/components/*"],
      "@/collections/*": ["../payload-cms/src/collections/*"],
      "@/utilities/*": ["../payload-cms/src/utilities/*"],
      "@/fields/*": ["../payload-cms/src/fields/*"],
      "@/hooks/*": ["../payload-cms/src/hooks/*"]
    },
    "rootDir": "src",
    "outDir": "dist"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
